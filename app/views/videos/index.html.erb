<% @videos.each do |video| %>
  <div class=" video">
    <h1><%= video.title %></h1>
    <embed src="<%= video.url%>" quality="high" width="700" height="500"  autostart="true" loop="-1" align="middle" allowScriptAccess="sameDomain" type="application/x-shockwave-flash"></embed>
    <br>
   <h3 style="margin-left:600px;"><%= link_to '编辑', edit_video_path(video) %>
   <%= link_to '删除', video, method: :delete, data: { confirm: '确定删除吗?' } %></h3>
  </div>
<% end %>


